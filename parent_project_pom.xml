    <!--*******-->
    <groupId>org.example</groupId>
    <artifactId>multi_module_project</artifactId>
    <packaging>pom</packaging>
    <version>1.0-SNAPSHOT</version>
    <modules>
        <module>manage_core</module> <!--project_module_name-->
        <module>manage_query</module> <!--project_module_name-->
        <module>manage_command</module> <!--project_module_name-->
